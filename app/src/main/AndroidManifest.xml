<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cystatusbar">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/CYAppTheme"
        android:name=".MyApplication"
        android:usesCleartextTraffic="true">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".spliteactivty.SplitActivity" android:documentLaunchMode="always" />
        <activity android:name=".spliteactivty.SplitMainActivity" />
        <activity android:name=".algorithm.AlgorithmActivity"></activity>
        <activity android:name=".OkHttpActivity"/>

    </application>

</manifest>